type User {
  id: ID!
  name: String!
  email: String!
  password: String!
  created_at: String!
  updated_at: String!
  posts: [Post!]!
}

type Post {
  id: ID!
  name: String!
  description: String!
  content: String!
  user: User!
  created_at: String!
  updated_at: String!
}

type Query {
  posts: [Post!]!
  users: [User!]!
}
